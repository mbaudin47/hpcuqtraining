\newcommand{\logophimeca}[1]{
\begin{tikzpicture}[#1, x=0.07cm, y=0.07cm]
  \fill[color = phimecadarkblue]
    (0,0) -- (0,16) -- (7.5,16)
    -- (7.5,16) arc (90:270:6)
    -- (7.5,4) -- (3,4)
    -- (3,4) arc (90:180:1)
    -- (2,3) -- (2,2) -- (13,2)
    -- (13,2) arc (-90:0:2)
    -- (15,4) -- plot [variable=\theta,domain=0:-90,smooth] ({13+2*cos(\theta)},{4+4*sin(\theta)})
    -- (13,0) -- (0,0) ;
  \fill[color = phimecadarkblue]
    (7.3,6.41242) -- (7.3,13.7)
    -- (7.3,13.7) arc (100:260:3.7)
    -- cycle ;
  \fill[color = phimecalightblue]
    (9.7,6.41242) -- (9.7,13.7)
    -- (9.7,13.7) arc (80:-80:3.7)
    -- cycle ;
  \fill[color = phimecalightblue]
    (17,20) -- (17,4) -- (9.5,4)
    -- (9.5,4) arc (-90:90:6)
    -- (9.5,16) -- (14,16)
    -- (14,16) arc (-90:0:1)
    -- (15,17) -- (15,18) -- (4,18)
    -- (4,18) arc (90:180:2)
    -- (2,16) -- plot [variable=\theta,domain=180:90,smooth] ({4+2*cos(\theta)},{16+4*sin(\theta)})
    -- (4,20) -- (17,20) ;
\end{tikzpicture}
}

